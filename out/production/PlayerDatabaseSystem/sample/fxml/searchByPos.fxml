<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.SearchByPos">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <AnchorPane prefHeight="500.0" prefWidth="750.0">
         <children>
            <Button layoutX="365.0" layoutY="344.0" mnemonicParsing="false" onAction="#searchButtonPos" prefHeight="76.0" prefWidth="186.0" text="Search">
               <font>
                  <Font size="30.0" />
               </font>
            </Button>
            <Label alignment="CENTER" contentDisplay="CENTER" layoutX="318.0" layoutY="14.0" prefHeight="68.0" prefWidth="281.0" text="Searching Player" textAlignment="CENTER" textFill="WHITE" underline="true">
               <font>
                  <Font size="36.0" />
               </font>
               <padding>
                  <Insets bottom="6.0" left="2.0" right="2.0" top="5.0" />
               </padding>
            </Label>
            <Button layoutX="765.0" layoutY="578.0" mnemonicParsing="false" onAction="#backButton" onDragDetected="#backButton" prefHeight="68.0" prefWidth="138.0" text="Back">
               <font>
                  <Font size="28.0" />
               </font>
            </Button>
            <Label alignment="CENTER" contentDisplay="CENTER" layoutX="311.0" layoutY="145.0" prefHeight="68.0" prefWidth="282.0" text="Enter Position" textAlignment="CENTER" textFill="#bec1bd" underline="true">
               <font>
                  <Font size="30.0" />
               </font>
               <padding>
                  <Insets bottom="6.0" left="2.0" right="2.0" top="5.0" />
               </padding>
            </Label>
            <ComboBox fx:id="comboBox" layoutX="295.0" layoutY="237.0" prefHeight="76.0" prefWidth="328.0" />
         </children>
      </AnchorPane>
   </children>
</GridPane>
